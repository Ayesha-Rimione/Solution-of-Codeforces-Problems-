#include<iostream>
using namespace std;
main()
{
    string s;
    cin>>s;
    int low=0,up=0;
    for(int i=0;i<s.size();i++)
    {
        if(islower(s[i]))
            low++;
        if(isupper(s[i]))
            up++;
    }
    if(up>low)
    {
        for(int i=0;i<s.size();i++)
    {
        if(islower(s[i]))
            s[i]-=32;

    }
    }
    else
    {
        for(int i=0;i<s.size();i++)
    {
        if(isupper(s[i]))
            s[i]+=32;

    }
    }
    cout<<s;


}
