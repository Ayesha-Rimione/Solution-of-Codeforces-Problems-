#include<iostream>
using namespace std;
main()
{
    int n,count=0,c=0;
    cin>>n;
    string s;
    cin>>s;
    for(int i=0;i<n;i++)
    {
        if(s[i]=='A')
            count++;
        else if(s[i]=='D')
            c++;
    }
    if(count>c)
        cout<<"Anton";
    else if ( count == c)
        cout<<"Friendship";
    else
        cout<<"Danik";
}

