#include<iostream>
using namespace std;
main()
{
    int n,as=0,bs=0,cs=0;
    cin>>n;
   for(int i=1;i<=n;i++)
    {
        int a,b,c;
        cin>>a>>b>>c;
        as+=a;
        bs+=b;
        cs+=c;

    }
    if(as==0 && bs==0 && cs==0)
        cout<<"YES";
        else
            cout<<"NO";

}
