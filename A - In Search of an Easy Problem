#include <iostream>
using namespace std;
main()
{
    int n;
    cin>>n;
    int a[n];
    int count=0;
    for(int i=0;i<n;i++)
       {
           cin>>a[i];
       }
        for(int i=0;i<n;i++)
        {
            if(a[i]==1)
                count++;
        }
    if(count==0)
        cout<<"EASY";
    else
        cout<<"HARD";

}
