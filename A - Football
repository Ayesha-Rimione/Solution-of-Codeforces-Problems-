#include<iostream>
#include<cstring>
using namespace std;
main()
{
   string s;
   cin>>s;
   if(s.size()<7)
    cout<<"NO";
   else
   {
       bool dan= false;
   for(int i=0;i<s.size()-6;i++)
   {
       if(s[i]=='0' && s.substr(i,7) == "0000000" ||
          s[i]=='1'&& s.substr(i,7) == "1111111"  )
            {dan=true;
       break;}
   }
   if(dan)
    cout<<"YES";
   else
    cout<<"NO";
   }

}
