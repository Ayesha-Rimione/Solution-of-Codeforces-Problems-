#include <bits/stdc++.h>
using namespace std;
main()
{
    long long int a,b,c,d,count=0;
    cin>>a>>b>>c>>d;
    if(a==b && b==c && c==d && a==d)
        cout<<"3";
    else if((a==b && b==c) || (b==c && c==d) || (a==c && c==d) || (b==c && a==d) || (a==c && b==d) ||(a==b && c==d) )
        cout<<"2";
    else if(a==b || b==c || c==d || d==a || b==d || a==c)
        cout<<"1";
    else
        cout<<"0";
}
