#include <bits/stdc++.h>
using namespace std;
main()
{
   int n;
   cin>>n;
   int p,q;
   cin>>p;
   int a[n+1]={0};
   int l;
   for(int i=0;i<p;i++)
   {
       cin>>l;
       a[l]=1;
   }
   cin>>q;
   for(int i=0;i<q;i++)
   {
       cin>>l;
       a[l]=1;
   }
   for(int i=1;i<=n;i++)
   {
       if(a[i]==0)
       {
           cout<<"Oh, my keyboard!";
           return 0;
       }
   }
   cout<<"I become the guy.";
}
