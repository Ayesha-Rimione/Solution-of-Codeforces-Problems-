#include<bits/stdc++.h>
using namespace std;
main()
{
    string s,k;
    cin>>s;
    for(int i=0;i<s.size();i++)
        s[i]=towlower(s[i]);
    for(int i=0;i<s.size();i++)
    {
        if(s[i]=='a' || s[i]=='o' ||s[i]=='y' ||s[i]=='e' ||s[i]=='u' ||s[i]=='i')
            continue;
        else{
            k+='.';
            k+=s[i];
        }
    }
    cout<<k;
}
