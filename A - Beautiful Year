#include<iostream>
using namespace std;
main()
{
   int a,b,c,d,ans,y;
   cin>>y;
   while(1)
   { 
       y++;
       ans=y;
   a=y%10;
   y/=10;
   b=y%10;
   y/=10;
   c=y%10;
   y/=10;
   d=y%10;
   if(a!=b && a!=c && a!=d && b!=c && b!=d && c!=d)
   {
       cout<<ans;
       break;}
       y=ans;
   }
   
}
